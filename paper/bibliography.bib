@Article{WOS:000734465000001,
  author                     = {Chataigner, Marc and Cousin, Areski and Crepey, Stephane and Dixon, Matthew and Gueye, Djibril},
  journal                    = {SIAM JOURNAL ON FINANCIAL MATHEMATICS},
  title                      = {Short Communication: Beyond Surrogate Modeling: Learning the Local Volatility via Shape Constraints},
  year                       = {2021},
  issn                       = {1945-497X},
  number                     = {3},
  pages                      = {SC58-SC69},
  volume                     = {12},
  abstract                   = {We explore the abilities of two machine learning approaches for
   no-arbitrage interpolation of European vanilla option prices, which
   jointly yield the corresponding local volatility surface: a finite
   dimensional Gaussian process (GP) regression approach under no-arbitrage
   constraints based on prices, and a neural net (NN) approach with
   penalization of arbitrages based on implied volatilities. We demonstrate
   the performance of these approaches relative to the SSVI industry
