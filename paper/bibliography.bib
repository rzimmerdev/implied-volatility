@Article{WOS:000734426800001,
  author                     = {Medvedev, Nikita and Wang, Zhiguang},
  journal                    = {JOURNAL OF FUTURES MARKETS},
  title                      = {Multistep forecast of the implied volatility surface using deep learning},
  year                       = {2022},
  issn                       = {0270-7314},
  month                      = {APR},
  number                     = {4},
  pages                      = {645-667},
  volume                     = {42},
  abstract                   = {Modeling implied volatility surface (IVS) is of paramount importance to
   price and hedge an option. We contribute to the literature by modeling
   the entire IVS using convolutional long-short-term memory (ConvLSTM) and
   long-short-term memory (LSTM) neural networks to produce multivariate
   and multistep forecasts of the S\&P 500 IVS. Using daily SPX options
   data (2002-2019), we find that both LSTM and ConvLSTM fit the training
   data extremely well with mean absolute percentage error (MAPE) being
   3.56\% and 3.88\%, respectively. The ConvLSTM (8.26\% MAPE) model
   significantly outperforms LSTM and traditional time series models in
   predicting the IVS out of sample.},
  address                    = {111 RIVER ST, HOBOKEN 07030-5774, NJ USA},
  affiliation                = {Wang, ZG (Corresponding Author), South Dakota State Univ, Ness Sch Management \& Econ, Brookings, SD 57007 USA. Medvedev, Nikita; Wang, Zhiguang, South Dakota State Univ, Ness Sch Management \& Econ, Brookings, SD 57007 USA.},
